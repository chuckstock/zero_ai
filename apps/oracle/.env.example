# Oracle Configuration
# Copy this to .env and fill in values

# Ethereum RPC endpoint (Sepolia)
RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# Or use Alchemy: https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Oracle private key (must match contract's authorized oracle)
ORACLE_PRIVATE_KEY=0x6eb278b33743de394bfc4c0a1db65dc498fd159f9118cad11af5e885995be213

# Contract address
CONTRACT_ADDRESS=0xD4Ffd32309dbB45F4F5cC153B6bAae5Cbb6d7443

# API server port (optional, for manual submission endpoint)
API_PORT=3000

# Auto-submit feedback to contract (true) or expose API only (false)
AUTO_SUBMIT=true

# Log level: error, warn, info, debug
LOG_LEVEL=info
